{
  "name": "ichhabrecht/git-checker",
  "description": "Web view for local git repository status",
  "keywords": [
    "Git",
    "Checker",
    "status"
  ],
  "homepage": "http://www.cps-it.de",
  "authors": [
    {
      "name": "Nicole Cordes",
      "role": "Developer",
      "homepage": "http://www.cps-it.de"
    }
  ],
  "license": [
    "GPL-2.0+"
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "GitChecker\\": "src/GitChecker/"
    }
  },
  "require": {
    "slim/slim": "^3.1",
    "slim/twig-view": "^2.0",
    "symfony/finder": "^3.0",
    "symfony/process": "^3.0",
    "symfony/yaml": "^3.0",
    "twbs/bootstrap": "^3.3"
  },
  "require-dev": {
    "composer/composer": "^1.0.0"
  },
  "scripts": {
    "post-install-cmd": "GitChecker\\Composer\\Script\\Installer::postInstall",
    "post-update-cmd": "GitChecker\\Composer\\Script\\Installer::postInstall"
  }
}
